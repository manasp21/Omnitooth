{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    }
  },
  "Omnitooth": {
    "Bluetooth": {
      "AdvertisingIntervalMs": 50
    },
    "Input": {
      "InputRateLimitMs": 0
    },
    "HID": {
      "ReportRateHz": 500
    },
    "Security": {
      "RequireAuthentication": false,
      "RequireEncryption": false
    },
    "Performance": {
      "GCLatencyMode": "LowLatency"
    }
  }
}